<Window x:Class="CaseControl.ClientInformation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Client Information" WindowState="Maximized" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" Height="918" Width="1022">
        <DockPanel>
        <Grid DockPanel.Dock="Left" Width="220" HorizontalAlignment="Left" >
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="3*" />
            </Grid.RowDefinitions>
            <TextBox Grid.Row="1" Text="Search" Margin="0,30,0,0" Name="txtSearch" TextChanged="txtSearch_TextChanged"></TextBox>
            <DataGrid AutoGenerateColumns="False" Grid.Row="2" Height="430" HorizontalAlignment="Left" Margin="0,05,0,0" Name="dgClientFileList" VerticalAlignment="Top" Width="210" SelectionChanged="dgClientFileList_SelectionChanged">
                <DataGrid.Columns>
                    <DataGridTextColumn Header="First Name" Binding="{Binding FirstName}"></DataGridTextColumn>
                    <DataGridTextColumn Header="Last Name" Binding="{Binding LastName}"></DataGridTextColumn>
                    <DataGridTextColumn Header="File No." Binding="{Binding FileID}"></DataGridTextColumn>
                </DataGrid.Columns>
            </DataGrid>
            <CheckBox Content="Show Inactive Clients" Grid.Row="3" Height="16" HorizontalAlignment="Left" Margin="0,10,0,0" Name="cbShowAllInactiveClients" VerticalAlignment="Top" Checked="cbShowAllInactiveClients_Checked" Unchecked="cbShowAllInactiveClients_Unchecked" />
        </Grid>
        <Grid DockPanel.Dock="Top" Height="30" VerticalAlignment="Top" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Button Grid.Row="1" Grid.Column="0" Content="Home Page" Margin="10,0,0,0"/>
            <Button Grid.Row="1" Grid.Column="1" Content="Add Client" Margin="10,0,0,0" Name="btnAddNewClient" Click="btnAddNewClient_Click" />
            <Button Grid.Row="1" Grid.Column="2" Content="Edit Client Information" Margin="10,0,0,0" />
            <Button Grid.Row="1" Grid.Column="3" Content="Save" Margin="10,0,0,0" Name="btnSaveClient" Click="btnSaveClient_Click" />
            <Button Grid.Row="1" Grid.Column="4" Content="Close Client" Margin="10,0,0,0"/>
            <Button Grid.Row="1" Grid.Column="5" Content="Re-Activate Closed Client" Margin="10,0,0,0"/>
            <Button Grid.Row="1" Grid.Column="6" Content="Expand All" Margin="10,0,0,0" Click="btnExpandCollapse_Click" Name="btnExpandCollapse"/>
        </Grid>
        <ScrollViewer Margin="0,20,0,0">
            <DockPanel>
                <Grid DockPanel.Dock="Top" Height="270">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="30"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="86" />
                        <ColumnDefinition Width="167" />
                        <ColumnDefinition Width="83" />
                        <ColumnDefinition Width="143" />
                        <ColumnDefinition Width="129" />
                        <ColumnDefinition Width="176" />
                    </Grid.ColumnDefinitions>
                    <Label Content="General Section" FontWeight="Bold" FontSize="13" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="6" Height="28" HorizontalAlignment="Left"  Name="label1" VerticalAlignment="Top" />
                    <Label Content="File No." Grid.Row="1" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="label10" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="1" Name="txtFileNo" VerticalAlignment="Top" Width="120" />
                    <Label Content="Last Name" Height="28" HorizontalAlignment="Left" Grid.Row="2" Grid.Column="0" Name="label11" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="2" Grid.Column="1" Name="txtLastName" VerticalAlignment="Top" Width="120" />
                    <Label Content="First Name" Height="28" HorizontalAlignment="Left" Grid.Row="3" Grid.Column="0" Name="label12" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="3" Grid.Column="1" Name="txtFirstName" VerticalAlignment="Top" Width="120" />
                    <Label Content="Case Type" Height="28" HorizontalAlignment="Left" Grid.Row="4" Grid.Column="0" Name="label13" VerticalAlignment="Top" />
                    <ComboBox Height="23" HorizontalAlignment="Left" Grid.Row="4" Grid.Column="1" Name="cmbCaseType" VerticalAlignment="Top" Width="120" />
                    <Label Content="Other Type" Height="28" HorizontalAlignment="Left" Grid.Row="5" Grid.Column="0" Name="label14" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="5" Grid.Column="1" Name="txtOtherCaseType" VerticalAlignment="Top" Width="120" />
                    <Label Content="Case Status" Height="28" HorizontalAlignment="Left" Grid.Row="6" Grid.Column="0" Name="label15" VerticalAlignment="Top" />
                    <ComboBox Height="23" HorizontalAlignment="Left" Grid.Row="6" Grid.Column="1" Name="cmbCaseStatus" VerticalAlignment="Top" Width="120">
                        <ComboBoxItem Content="Open" />
                        <ComboBoxItem Content="Closed" />
                    </ComboBox>
                    <Label Content="Accident Date" Height="28" HorizontalAlignment="Left" Grid.Row="7" Grid.Column="0" Name="label16" VerticalAlignment="Top" />
                    <DatePicker Height="25" HorizontalAlignment="Left" Grid.Row="7" Grid.Column="1" Name="dtAccidentDate" VerticalAlignment="Top" Width="120" />
                    <Label Content="Address" Grid.Row="1" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="label17" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="3" Name="txtAddress" VerticalAlignment="Top" Width="120" />
                    <Label Content="City" Grid.Row="2" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="label18" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="2" Grid.Column="3" Name="txtCity" VerticalAlignment="Top" Width="120" />
                    <Label Content="State" Grid.Row="3" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="label19" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="3" Grid.Column="3" Name="txtState" VerticalAlignment="Top" Width="120" />
                    <Label Content="Home Phone" Grid.Row="4" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="label20" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="4" Grid.Column="3" Name="txtHomePhone" VerticalAlignment="Top" Width="120" />
                    <Label Content="Cell Phone" Grid.Row="5" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="label21" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="5" Grid.Column="3" Name="txtCellPhone" VerticalAlignment="Top" Width="120" />
                    <Label Content="Driving License" Grid.Row="6" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="label22" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="6" Grid.Column="3" Name="txtDrivingLicense" VerticalAlignment="Top" Width="120" />
                    <Label Content="Date Of Birth" Grid.Row="7" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="label23" VerticalAlignment="Top" />
                    <DatePicker Height="25" HorizontalAlignment="Left" Grid.Row="7" Grid.Column="3" Name="dtDateOfBirth" VerticalAlignment="Top" Width="120" />
                    <Label Content="SSN" Grid.Row="8" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="label24" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="8" Grid.Column="3" Name="txtSSN" VerticalAlignment="Top" Width="120" />
                    <Label Content="Intial Case Information" Grid.Row="1" Grid.Column="4" Height="28" HorizontalAlignment="Left"  Name="label25" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="5" Name="txtInitialCaseInformation" VerticalAlignment="Top" Width="120" />
                    <Label Content="Defendant Name" Grid.Row="2" Grid.Column="4" Height="28" HorizontalAlignment="Left"  Name="label26" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="2" Grid.Column="5" Name="txtDefendantName" VerticalAlignment="Top" Width="120" />
                    <Label Content="Evidence 1" Grid.Row="3" Grid.Column="4" Height="28" HorizontalAlignment="Left"  Name="label27" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="3" Grid.Column="5" Name="txtEvidence1" VerticalAlignment="Top" Width="120" />
                    <Label Content="Evidence 2" Grid.Row="4" Grid.Column="4" Height="28" HorizontalAlignment="Left"  Name="label28" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="4" Grid.Column="5" Name="txtEvidence2" VerticalAlignment="Top" Width="120" />
                    <Button Content="View All Evidence" Grid.Row="5" Grid.Column="5" Height="23" HorizontalAlignment="Left" Name="btnViewAllEvidence" VerticalAlignment="Top" Width="122" Click="btnViewAllEvidence_Click" />
                    <Label Content="Originating Attorney" Grid.Row="6" Grid.Column="4" Height="28" HorizontalAlignment="Left"  Name="label29" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="6" Grid.Column="5" Name="txtOriginatingAttorney" VerticalAlignment="Top" Width="120" />
                    <Label Content="Assigned Attorney" Grid.Row="7" Grid.Column="4" Height="28" HorizontalAlignment="Left"  Name="label30" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="7" Grid.Column="5" Name="txtAssignedAttorney" VerticalAlignment="Top" Width="120" />
                    <Label Content="Refferal" Grid.Row="8" Grid.Column="4" Height="28" HorizontalAlignment="Left"  Name="label31" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="8" Grid.Column="5" Name="txtRefferal" VerticalAlignment="Top" Width="120" />
                </Grid>
                <Expander Margin="0,10,0,0" Header="Additional Client Information" DockPanel.Dock="Top" VerticalAlignment="Top"  Name="exdAdditionalClientInformation"  FontWeight="Bold" FontSize="13">
                    <Expander.Resources>
                        <Style TargetType="{x:Type TextBox}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                        <Style TargetType="{x:Type Label}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                        <Style TargetType="{x:Type DatePicker}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                    </Expander.Resources>
                    <Grid Height="248" VerticalAlignment="Top">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="95" />
                            <ColumnDefinition Width="129" />
                            <ColumnDefinition Width="2*" />
                        </Grid.ColumnDefinitions>
                        <Label Content="Client Occupation" Grid.Row="0" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientOccupation" VerticalAlignment="Top" />
                        <TextBox Height="23" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientOccupation" VerticalAlignment="Top" Width="120" />
                        <Label Content="Employer" Grid.Row="1" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblEmployer" VerticalAlignment="Top" />
                        <TextBox Height="23" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left"  Name="txtEmployer" VerticalAlignment="Top" Width="120" />
                        <Label Content="Address" Grid.Row="2" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblAddress" VerticalAlignment="Top" />
                        <TextBox Height="23" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientEmployerAddress" VerticalAlignment="Top" Width="120" />
                        <Label Content="City" Grid.Row="3" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblCity" VerticalAlignment="Top" />
                        <TextBox Height="23" Grid.Row="3" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientEmployerCity" VerticalAlignment="Top" Width="120" />
                        <Label Content="State" Grid.Row="4" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblState" VerticalAlignment="Top" />
                        <TextBox Height="23" Grid.Row="4" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientEmployerState" VerticalAlignment="Top" Width="120" />
                        <Expander Header="Spouse Information" Grid.Row="0" Grid.Column="2" Name="exdSpouseInfo"  Grid.RowSpan="8" FontWeight="Bold" FontSize="13">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="95" />
                                    <ColumnDefinition Width="129" />
                                </Grid.ColumnDefinitions>
                                <Label Content="Last Name" Grid.Row="0" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblSpouseLastName" VerticalAlignment="Top" />
                                <TextBox Height="23" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientSpousLastName" VerticalAlignment="Top" Width="120" />
                                <Label Content="First Name" Grid.Row="1" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblSpouseFirstName" VerticalAlignment="Top" />
                                <TextBox Height="23" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientSpousFirstName" VerticalAlignment="Top" Width="120" />
                                <Label Content="Spouse Occupation" Grid.Row="2" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientSpousOccupation" VerticalAlignment="Top" />
                                <TextBox Height="23" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientSpousOccupation" VerticalAlignment="Top" Width="120" />
                                <Label Content="Employer" Grid.Row="3" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblSpouseEmployer" VerticalAlignment="Top" />
                                <TextBox Height="23" Grid.Row="3" Grid.Column="1" HorizontalAlignment="Left"  Name="txtSpouseEmployer" VerticalAlignment="Top" Width="120" />
                                <Label Content="Address" Grid.Row="4" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblSpouseAddress" VerticalAlignment="Top" />
                                <TextBox Height="23" Grid.Row="4" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientSpouseEmployerAddress" VerticalAlignment="Top" Width="120" />
                                <Label Content="City" Grid.Row="5" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblSpouseCity" VerticalAlignment="Top" />
                                <TextBox Height="23" Grid.Row="5" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientSpouseEmployerCity" VerticalAlignment="Top" Width="120" />
                                <Label Content="State" Grid.Row="6" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblSpouseState" VerticalAlignment="Top" />
                                <TextBox Height="23" Grid.Row="6" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientSpouseEmployerState" VerticalAlignment="Top" Width="120" />
                            </Grid>
                        </Expander>
                    </Grid>
                </Expander>
                <Expander Margin="0,10,0,0" Header="Client Auto Information" DockPanel.Dock="Top"  Name="exdClientAutoInformation" FontWeight="Bold" FontSize="13">
                    <Expander.Resources>
                        <Style TargetType="{x:Type TextBox}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                        <Style TargetType="{x:Type Label}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                        <Style TargetType="{x:Type DatePicker}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                    </Expander.Resources>
                    <Grid >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="110" />
                                <ColumnDefinition Width="167" />
                                <ColumnDefinition Width="150" />
                                <ColumnDefinition Width="143" />
                            </Grid.ColumnDefinitions>
                            <Label Content="Insurance Company" Grid.Row="0" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblInsuranceCompany" VerticalAlignment="Top" />
                            <TextBox Height="23" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientAutoInsuranceCompany" VerticalAlignment="Top" Width="120" />
                            <Label Content="Address" Grid.Row="1" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientAutoAddress" VerticalAlignment="Top" />
                            <TextBox Height="23" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientAutoAddress" VerticalAlignment="Top" Width="120" />
                            <Label Content="City" Grid.Row="2" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientAutoCity" VerticalAlignment="Top" />
                            <TextBox Height="23" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientAutoCity" VerticalAlignment="Top" Width="120" />
                            <Label Content="State" Grid.Row="3" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientAutoState" VerticalAlignment="Top" />
                            <TextBox Height="23" Grid.Row="3" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientAutoState" VerticalAlignment="Top" Width="120" />
                            <Label Content="Zip" Grid.Row="4" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientAutoZip" VerticalAlignment="Top" />
                            <TextBox Height="23" Grid.Row="4" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientAutoZip" VerticalAlignment="Top" Width="120" />
                            <Label Content="Phone Number" Grid.Row="5" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientAutoPhoneNumber" VerticalAlignment="Top" />
                            <TextBox Height="23" Grid.Row="5" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientAutoPhoneNumber" VerticalAlignment="Top" Width="120" />
                            <Label Content="Adjuster" Grid.Row="6" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientAutoAdjuster" VerticalAlignment="Top" />
                            <TextBox Height="23" Grid.Row="6" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientAutoAdjuster" VerticalAlignment="Top" Width="120" />
                            <Label Content="Policy Number" Grid.Row="0" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblPolicyNumber" VerticalAlignment="Top" />
                            <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="0" Grid.Column="3" Name="txtPolicyNumber" VerticalAlignment="Top" Width="120" />
                            <Label Content="Effective Start Date" Grid.Row="1" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblEffectiveStartDate" VerticalAlignment="Top" />
                            <DatePicker Height="25" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="4" Name="dtEffectiveStartDate" VerticalAlignment="Top" Width="120" />
                            <Label Content="Effective End Date" Grid.Row="2" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblEffectiveEndDate" VerticalAlignment="Top" />
                            <DatePicker Height="25" HorizontalAlignment="Left" Grid.Row="2" Grid.Column="4" Name="dtEffectiveEndtDate" VerticalAlignment="Top" Width="120" />
                            <Label Content="Med Pay Available" Grid.Row="3" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblMedPayAvailable" VerticalAlignment="Top" />
                            <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="3" Grid.Column="3" Name="txtMedPayAvailable" Text="N" VerticalAlignment="Top" Width="120" MaxLength="1" />
                            <Label Content="Amount" Grid.Row="4" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblAmount" VerticalAlignment="Top" />
                            <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="4" Grid.Column="3" Name="txtAmount" VerticalAlignment="Top" Width="120" />
                            <Label Content="Liability Minimum Coverage" Grid.Row="5" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblLiabilityMinimumCoverage" VerticalAlignment="Top" />
                            <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="5" Grid.Column="3" Name="txtLiabilityMinimumCoverage" VerticalAlignment="Top" Width="120" />
                            <Label Content="Liability Maximum Coverage" Grid.Row="6" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblLiabilityMaximumCoverage" VerticalAlignment="Top" />
                            <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="6" Grid.Column="3" Name="txtLiabilityMaximumCoverage" VerticalAlignment="Top" Width="120" />
                            <Label Content="UMI Minimum" Grid.Row="7" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblUMIMinimum" VerticalAlignment="Top" />
                            <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="7" Grid.Column="3" Name="txtUMIMinimum" VerticalAlignment="Top" Width="120" />
                            <Label Content="UMI Maximum" Grid.Row="8" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblUMIMaximum" VerticalAlignment="Top" />
                            <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="8" Grid.Column="3" Name="txtUMIMaximum" VerticalAlignment="Top" Width="120" />
                            <Label Content="Reimbursable" Grid.Row="9" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblReimbursable" VerticalAlignment="Top" />
                            <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="9" Grid.Column="3" Name="txtReimbursable" Text="N" VerticalAlignment="Top" Width="120" />
                            <Label Content="Notes" Grid.Row="10" Grid.Column="2" Height="28" HorizontalAlignment="Left"  Name="lblClientAutoInfoNotes" VerticalAlignment="Top" />
                            <TextBox Height="23" HorizontalAlignment="Left" Grid.Row="10" Grid.Column="3" Name="txtClientAutoInfoNotes" VerticalAlignment="Top" Width="120" />
                        </Grid>
                    </Grid>
                </Expander>
                <Expander Margin="0,10,0,0" Header="Client Medical Insurance" DockPanel.Dock="Top"  Name="exdClientMedicalInsurance" FontWeight="Bold" FontSize="13">
                    <Expander.Resources>
                        <Style TargetType="{x:Type TextBox}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                        <Style TargetType="{x:Type Label}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                        <Style TargetType="{x:Type DatePicker}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                        <Style TargetType="{x:Type CheckBox}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                        <Style TargetType="{x:Type DataGrid}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                        <Style TargetType="{x:Type Button}">
                            <Setter Property="FontWeight" Value="Normal"></Setter>
                            <Setter Property="FontSize" Value="11"></Setter>
                        </Style>
                    </Expander.Resources>
                    <Grid >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="110" />
                            <ColumnDefinition Width="130" />
                            <ColumnDefinition Width="110" />
                            <ColumnDefinition Width="130" />
                            <ColumnDefinition Width="4*" />
                        </Grid.ColumnDefinitions>
                        <Label Content="Named Insured" Grid.Row="0" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblNamedInsured" VerticalAlignment="Top" />
                        <TextBox Height="23" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left"  Name="txtNamedInsured" VerticalAlignment="Top" Width="120" />
                        <Label Content="Insurance Company" Grid.Row="1" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientMedicalInsuranceCompany" VerticalAlignment="Top" />
                        <TextBox Height="23" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left"  Name="txtInsuranceCompany" VerticalAlignment="Top" Width="120" />
                        <Label Content="Address" Grid.Row="2" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientMedicalAddress" VerticalAlignment="Top" />
                        <TextBox Height="23" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientMedicalAddress" VerticalAlignment="Top" Width="120" />
                        <Label Content="City" Grid.Row="3" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientMedicalCity" VerticalAlignment="Top" />
                        <TextBox Height="23" Grid.Row="3" Grid.Column="1" HorizontalAlignment="Left"  Name="txtClientMedicalCity" VerticalAlignment="Top" Width="120" />
                        <Label Content="State" Grid.Row="4" Grid.Column="0" Height="28" HorizontalAlignment="Left"  Name="lblClientMedicalState" VerticalAlignment="Top" />
